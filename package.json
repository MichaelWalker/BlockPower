{
    "name": "blockpower",
    "version": "0.1.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "dev:device": "expo start --clear --tunnel",
        "dev:emulator": "expo start --ios --clear --localhost",
        "lint": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix --max-warnings=0",
        "publish:preview": "eas build --platform ios --profile preview",
        "publish:production": "eas build --platform ios --profile preview",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "typecheck": "tsc --noEmit --project ./tsconfig.json",
        "prepare": "husky install",
        "prettier:check": "prettier --check .",
        "prettier:write": "prettier --write .",
        "pretty-quick": "pretty-quick --staged"
    },
    "dependencies": {
        "@expo-google-fonts/lato": "^0.2.2",
        "expo": "~44.0.0",
        "expo-app-loading": "^1.3.0",
        "expo-status-bar": "~1.2.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-native": "0.64.3",
        "react-native-web": "0.17.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.16.11",
        "@testing-library/react-native": "^9.0.0",
        "@types/jest": "^27.4.1",
        "@types/react": "~17.0.21",
        "@types/react-native": "~0.64.12",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "eslint": "^8.11.0",
        "eslint-plugin-react": "^7.29.3",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-react-native": "^4.0.0",
        "husky": "^7.0.0",
        "jest": "^27.5.1",
        "jest-expo": "^44.0.1",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "react-test-renderer": "^17.0.2",
        "typescript": "~4.3.5"
    },
    "private": true,
    "jest": {
        "preset": "jest-expo"
    }
}
